{
    "function": [
        "function {name} ({arguments}) {",
        "{body}",
        "}\n\n"
    ],
    "method": [
        "{name}: function ({arguments}) {",
        "{body}",
        "},\n\n"
    ],
    "classMethod": [
        "{name} ({arguments}) {",
        "{body}",
        "}\n\n"
    ],
    "functionCall": [
        "{name}({arguments})"
    ],
    "methodCall": [
        "this.{name}({arguments})"
    ]
}