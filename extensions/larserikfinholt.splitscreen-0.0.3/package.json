{
	"name": "splitscreen",
	"displayName": "SplitScreen",
	"description": "SplitsScreen verticaly between javascript, html and css. Works on Vue, Angular, Aurelia, and others",
	"version": "0.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/larserikfinholt/splitscreen"
	},
	"publisher": "larserikfinholt",
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.splitScreen2",
		"onCommand:extension.splitScreen3"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.splitScreen2",
				"title": "SplitScreen js | html"
			},
			{
				"command": "extension.splitScreen3",
				"title": "SplitScreen js | html | css"
			}
		],
		"keybindings": [
			{
				"command": "extension.splitScreen2",
				"key": "ctrl+i 2",
				"mac": "cmd+i 2",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.splitScreen3",
				"key": "ctrl+i 3",
				"mac": "cmd+i 3",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"keywords": [
		"Vue",
		"Aurelia",
		"Angular",
		"Views",
		"Split"
	],
	"__metadata": {
		"id": "c8f64a2a-67e4-4fdf-b21e-94cf9b772944",
		"publisherId": "81a9f96c-1ad8-482a-9cd5-d562121a4e2f",
		"publisherDisplayName": "Lars Erik Finholt"
	}
}