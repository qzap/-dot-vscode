{
	"name": "npm-scripts",
	"displayName": "NPM Scripts",
	"description": "Easy access to your project's npm scripts",
	"version": "1.0.0",
	"publisher": "lukaserat",
	"galleryBanner": {
		"color": "#000000",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"npm scripts",
		"scripts",
		"npm run",
		"npm",
		"nodejs",
		"listing",
		"execute scripts",
		"script listing"
	],
	"activationEvents": [
		"workspaceContains:**/package.json"
	],
	"main": "./out/extension",
	"icon": "images/logo.png",
	"homepage": "https://github.com/lukaserat/vscode-npmscripts/blob/master/README.md",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/lukaserat/vscode-npmscripts.git"
	},
	"bugs": {
		"url": "https://github.com/lukaserat/vscode-npmscripts/issues"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.npmScripts",
				"title": "NPM Scripts"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/lodash": "^4.14.118",
		"lodash": "^4.17.11"
	},
	"__metadata": {
		"id": "29d1fd28-6c4a-45a1-8b35-0460260be12e",
		"publisherId": "8dff2035-ed42-4edd-8cdf-80d7e2b979eb",
		"publisherDisplayName": "lukaserat"
	}
}