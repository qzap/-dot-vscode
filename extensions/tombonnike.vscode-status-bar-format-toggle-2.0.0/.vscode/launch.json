{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Launch VSCode with the extension installed",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--disable-extensions",
        "--extensionDevelopmentPath=${workspaceRoot}"
      ],
      "outFiles": ["${workspaceRoot}/out/**/*.js"]
    }
  ]
}
