{
	"shelf-controller": {
		"prefix": "fu-shelf-controller",
		"body": [
			"import 'dart:async';",
			"import 'dart:convert';",
			"import 'package:shelf/shelf.dart';",
			"import 'package:shelf_router/shelf_router.dart';",
			"",
			"part '${TM_FILENAME_BASE}.g.dart';",
			"",
			"class ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}} {",
			"",
			"   @Route.get('/')",
            "   Future<Response> find(Request request) async { ",
			"      return Response.ok(jsonEncode(''));",
			"   }",
			"",
			"   Router get router => _$${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}}Router(this);",
			"}"
		],
		"description": "Create controller shelf"
    },
    "shelf-operation": {
		"prefix": "fu-shelf-operation",
		"body": [
            "@Route.$1('$2')",
            "Future<Response> $3(Request request) async{",
			"   return Response.ok(jsonEncode(''));",
			"}"
		],
		"description": "Create shelf operation"
    }
}