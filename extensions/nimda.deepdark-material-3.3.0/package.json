		"eslint-plugin-json": "1.4.0",
		"eslint-plugin-prettier": "2.6.1",
		"prettier": "1.13.7",
		"remark-cli": "6.0.1",
		"remark-preset-lint-arcticicestudio": "0.1.0"
	}
